<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.washington.cathej.quizdroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MathOverActivity" />
        <activity android:name=".MathQuizActivity" />
        <activity android:name=".PhysicsOverActivity" />
        <activity android:name=".MarvelOverActivity" />
        <activity android:name=".PotterOverActivity" />
        <activity android:name=".PhysicsQuizActivity" />
        <activity android:name=".MarvelQuizActivity" />
        <activity android:name=".PotterQuizActivity"></activity>
    </application>

</manifest>